<%- include header %>
<form class="form" style="padding-top:70px;" method="post">
	<div class="form-group">
	    <label for="regUsername" class="control-label">用户名: </label>
		<input id="regUsername" name="username" type="text" class="form-control input-sm">
	</div>
	<div class="form-group">
	    <label class="control-label" for="regPassword">密码: </label>
		<input id="regPassword" name="password" type="password" class="form-control">
	</div>
	<div class="form-group">
	    <label class="control-label" for="regPassword">再次输入密码: </label>
		<input id="regPassword" name="passwordRepeat" type="password" class="form-control">
	</div>
	<button type="submit" class="btn btn-primary">提交</button>
</form>
<%- include footer %>

